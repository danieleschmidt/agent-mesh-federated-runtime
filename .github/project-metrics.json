  "automation": {
    "collection": {
      "frequency": "daily",
      "retention_days": 365,
      "storage": "github_repository",
      "format": "json"
    },
    "alerts": {
      "enabled": true,
      "channels": ["slack", "email", "github_issues"],
      "severity_levels": ["critical", "warning", "info"]
    },
    "reporting": {
      "dashboard_url": "https://grafana.agent-mesh.com/d/project-metrics",
      "weekly_report": true,
      "monthly_summary": true,
      "quarterly_review": true
    },
    "integrations": {
      "github": {
        "enabled": true,
        "metrics": ["commits", "prs", "issues", "releases", "contributors"]
      },
      "sonarcloud": {
        "enabled": false,
        "project_key": "agent-mesh-federated-runtime",
        "metrics": ["coverage", "duplication", "maintainability", "reliability", "security"]
      },
      "prometheus": {
        "enabled": true,
        "endpoint": "https://prometheus.agent-mesh.com",
        "metrics": ["performance", "reliability", "availability"]
      },
      "sentry": {
        "enabled": false,
        "project": "agent-mesh",
        "metrics": ["errors", "performance", "releases"]
      }
    }
